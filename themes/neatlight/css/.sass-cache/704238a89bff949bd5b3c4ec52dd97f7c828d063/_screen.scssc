/* - General HTML elements - */

html{
 height: 100%;   
}

* {
  box-sizing: border-box;
  -webkit-box-sizing: border-box;
  -moz-box-sizing: border-box;
  ::selection {
    background: $red;
    color: #fff;
  }
}

.clear {
    clear:both;
}

.clearfix:before,
.clearfix:after {
    content: " "; 
    display: table; 
}

.clearfix:after {
    clear: both;
}

.clearfix {
    *zoom: 1;
}

body {
    margin: 0;
    padding-left: 20px;
    padding-right: 20px;
    background: $almost-white;
    overflow-y: scroll;
    height: 100%;
    
    p {
    max-width: 75%;
    }
}

.container {
    position: relative;
    min-height: 100%;
}

#header {
  height: 50px;
  width: 100%;
}

.nav {
  background-color: #ca3838;
  position: fixed;
  top: 0;
  left: 0;
  right: auto;
  z-index: 7000;
  @include box-shadow(0px, 3px, 8px, rgba(0, 0, 0, 0.25)); 
  width: 100%;
  max-height: 50px;
  padding: 5px 0px;
    
  img {
    display: inline-block;
    float: right;
    margin-top: 5px;
    margin-right: 10px;
  }
  h1 {
    display: inline;
    color: white;
    padding: 0px 12px;
    font-size: 1.8em;
    margin: 0px;
    font-weight: 400;
  }
  a {
    color: white;
    font-size: 1.25em;
    font-weight: 400;
  }
  ul {
    padding: 5px 15px 5px 5px;
    margin: 0px;
    float: right;
  }
  li {
    display: inline;
    padding: 0px 8px;
  }
}

#primary {
    padding-bottom: 125px;   
}

 #footer {
     height: 125px;
     position: absolute;
    bottom: 0;
    left: 0;
    display: block;
    width: 100%;
    clear: both;
     padding-bottom: 15px;
     
    img {
      float: left;
      //margin-left: 20px;
      max-width: initial;
       max-height: initial;
    }
     .adr{
     float: right;
     display: inline-block;
     padding-top: 5px;
     padding-bottom: 5px;
     margin: 0 50px 0px 0;
     }
     
}

/* -- Text -- */

body {
    font-family: "Roboto Condensed";
    font-size: 1em;
    font-weight: 300;
    
}

.fa {
  font-size: 14px;
}

strong {
    font-weight: 400; 
}

/* --- Headings --- */

h1 {
  font-family: "Roboto Condensed";
}

h2 {
  font-weight: 400;
  display: inline-block;
}

h3 {
  display: block;
  font-size: 1.125em;
  line-height: 1.125em;
  -webkit-margin-before: 0px;
  -webkit-margin-after: 0px;
  -webkit-margin-start: 0px;
  -webkit-margin-end: 0px;
  font-weight: 500;
  margin-top: 0;
  margin-bottom: 0;
}

/* --- Links --- */

a {
  text-decoration: none;
  color: $red;
  &:hover {
    text-decoration: underline;
  }
}

.fa-external-link:before,
.fa-globe:before {
  color: $red;
}

/* - Neatline exhibit styling - */

body.neatline.show {
    
    padding: 0;
    
	div.exhibit {
	position: absolute;
	top: 0;
	left: 0;
	width: 100%;
	right: auto;
		#neatline, #neatline-map {
            height: 100%;
          }
        
        /* Open Layers Map Controls */

#OpenLayers_Control_PanZoom_2 {
  top: 54px !important;
}

.olControlLayerSwitcher {
  .maximizeDiv, .minimizeDiv {
    display: none;
  }
}

#neatline.Waypoints.Simile .olControlLayerSwitcher {
  right: 0;
}
	}
}

/*Pop-up bubble styling */ 

#static-bubble img {
  max-width: 40%;
}

#static-bubble {
    &.bound, &.selected {
    overflow-y: auto;
    position: absolute;
    background: $almost-white;
    @include border-radius(2px);
    color: #000;
    padding: 1em;
    max-height: 75%;
    width: 50%;
    max-width: 400px;
    right: 1.5em;
    left: auto;
    top: 4em;
    font-size: 1em;
    z-index: 500;
    opacity: 1;
    @include box-shadow(0px, 3px, 8px, rgba(0, 0, 0, 0.25));
    .content {
        display: block;
        margin-top: .5em;
        } 
    }
    .title {
        display: inline;
        font-size: 1.25em;
        font-weight: 400;
        line-height: 1em;
        }
    .smaller_text {
        font-size: .9em;
        }
    a.close {
        font-size: 2em;
        line-height: .8em;
        }
    hr, &.selected hr {
        display: none !important;
        }
    .item-file {
        display: inline;
        .thumb {
        max-width: 40% !important;
        display: inline;
        }
    }
    
    
}




/* --- Simile Timeline ---  */ 

/* Positioning and Styling Simile Timeline */

#neatline {
  #simile, &.Waypoints.Simile #simile {
    height: 100px;
    width: 40%;
    bottom: 10px;
    right: 10px;
    left: auto;
    opacity: 1;
    @include box-shadow(0px, 3px, 8px, rgba(0, 0, 0, 0.25));
    z-index: 998;
    border-bottom: 5px solid black;
    cursor: url(/themes/neatlight/images/grab.ico);
    cursor: -webkit-grab;
    cursor: -moz-grab !important;
    cursor: grab;
      &:active {
    cursor: -webkit-grabbing;
    cursor: -moz-grabbing;
    cursor: grabbing;
    }
  }
}

.timeline-band-layer-inner:active {
  cursor: url(/themes/neatlight/images/grabbing.ico);
  cursor: -webkit-grabbing;
  cursor: -moz-grabbing !important;
  cursor: grabbing;
}

#simile *::selection,
#simile::selection {
    background: rgba(0,0,0,0);
    color: #000;
}

